/* bottom-top Border*/
.border-top {
  position: relative;
  max-width: 1px;
  background-color: #afafaf;
  text-align: center;
  margin: 0 auto 0px;
  height: 150px;
}
.border-top.animate {
  transition: 10s;
  animation-delay: 200ms;
}
.border-top .border-outer {
  display: block;
  max-width: 1px;
  height: 150px;
}
.border-top .border-inner {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #1c1c1c;
  overflow: hidden;
  height: 0;
  transition: .9s ease;
}
.border-top.animated .border-inner {
  top: 0;
  height: 100%;
}
 .border-top.border-top-light .border-inner {
  background-color: #fff;
 }
 
/* Left-right border*/
.border-hover {
  position: relative;
  width: 130px;
  background-color: #afafaf;
  margin-left: 10px;
}
.border-hover .border-outer {
  display: block;
  height: 2px;
}
.border-hover  .border-inner {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  background-color: #000;
  overflow: hidden;
  width: 0px;
  height: 2px;
  transition: .5s ease;
  margin: 0;
}
.btn-hover-animation:hover .border-inner {
  right: 1px;
  width: 100%;
}
.border-hover.border-light .border-inner{
  background-color: #F5F4ED;
}
