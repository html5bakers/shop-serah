/* Animation */
html.js .animate{
  position: relative;
  opacity: 0;
}
html.js .animated{
  opacity: 1;
  transition: .5s;
  -webkit-animation-delay: 20ms;
  animation-delay: 20ms;
}
html.js .show_up{ top: 20px; }
html.js .show_up.animated{
  top: 0;
}
html.js .slide_left{ right: -20px; }
html.js .slide_left.animated{ right: 0px; }
html.js .slide_right{ left: -20px; }
html.js .slide_right.animated{ left: 0px; }

html.js .animated.animate-1{
  transition: 1s;
  -webkit-animation-delay: 30ms;
  transition-delay: 30ms;
}
html.js .animated.animate-1-5{
  transition: 1.5s;
  -webkit-animation-delay: 60ms;
  animation-delay: 60ms;
}
html.js .animated.ad-2{
  transition: 2s;
  -webkit-animation-delay: 90ms;
  animation-delay: 90ms;
}
html.js .animated.ad-2-5 {
  transition: 2.5s;
  -webkit-animation-delay: 110ms;
  animation-delay: 110ms;
}
html.js .animated.ad-3 {
  transition: 3s;
  -webkit-animation-delay: 120ms;
  animation-delay: 120ms;
}
body.overflow-hidden{
