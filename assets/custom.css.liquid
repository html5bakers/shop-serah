  justify-content: center;
  height: 100%;
}
.loader .svg-wrap{
  max-width: 500px;
  opacity: .9;
  transition: all .5s;
}
.loader.scale .svg-wrap{
  transform: scale(1.2);
  opacity: 1;
  transition: all 1s;
}

